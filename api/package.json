{
  "name": "ai-app",
  "version": "1.0.0",
  "description": "Boilerplate for Typescript Node application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "cli": "ts-node src/cli.ts",
    "dev": "tsc --project tsconfig.json --watch & NODE_ENV=development nodemon dist",
    "run-job-runner": "ts-node --transpile-only ./src/index-job-runner.ts",
    "prestart": "npm run build",
    "start": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "test-api-lambda": "ts-node --transpile-only ./src/test-api-lambda.ts",
    "test-job-runner": "ts-node --transpile-only ./src/test-job-runner.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenley/ai-app"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.632.0",
    "@vendia/serverless-express": "^4.12.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "openai": "^4.56.0",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "jest": "^27.5.1",
    "nodemon": "^3.1.4",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
